{
  "name": "youtube-api",
  "displayName": "YouTube API",
  "description": "YouTube Data API for video research, channel analytics, and content discovery",
  "version": "1.0.0",
  "capabilities": [
    {
      "category": "social_youtube",
      "defaultPriority": 1,
      "useCases": ["video_research", "channel_analytics", "content_discovery", "trend_analysis"]
    }
  ],
  "integrationType": "mcp",
  "scriptPath": "services/youtube-api/query.py",
  "testPath": "services/youtube-api/test.py",
  "mcp": {
    "serverName": "youtube",
    "envMapping": {
      "YOUTUBE_API_KEY": "${YOUTUBE_API_KEY}"
    }
  },
  "selfHosted": null,
  "auth": {
    "type": "api_key",
    "envVars": ["YOUTUBE_API_KEY"],
    "primaryEnvVar": "YOUTUBE_API_KEY",
    "docsUrl": "https://developers.google.com/youtube/v3/getting-started"
  },
  "costInfo": {
    "summary": "Free: 10,000 units/day",
    "freeTier": true
  }
}
